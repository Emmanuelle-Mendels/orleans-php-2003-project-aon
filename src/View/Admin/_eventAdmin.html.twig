<section class="cookies container-fluid">
    <div class="row">
        <h2>Event Admin</h2>


        <table class="table table-stripped">
            <thead>
            <tr>
                <th>Event</th>
                <th>Date</th>
                <th>location</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>
            </thead>
            <tbody>
            {% for id in event %}
                <tr>
                    <td>{{ id.title }}</td>
                    <td>{{ id.date }}</td>
                    <td>{{ id.location }}</td>
                    <td>

                    </td>
                    <td>
                        <button>
                            <a href="delete/{{ id.id }}"><img class="delete_button" src="/./assets/icons/logo_delete.png" alt="logo_delete"></a>
                        </button>
                    </td>
                </tr>
            {% endfor %}

            </tbody>
        </table>
    </div>
</section>
